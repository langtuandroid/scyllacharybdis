<?xml version="1.0" encoding="utf-8" ?>
<spritesheet>
	<texture filename="test.png">asdf</texture>
	<bodies>
		<body name="spinnerbody" dynamtic="true">
			<polygon top="0.0" left="0.0" width="1.0" height="1.0" friction="0.3" density="0" restitution="0.1"/>
		</body>
		<body name="bouncybody" dynamtic="true">
			<polygon top="0.0" left="0.0" width="1.0" height="1.0" friction="0.3" density="0" restitution="0.5"/>
		</body>
	</bodies>
	<areas>
		<area name="spinnersprite" topleft="0,0" bottomright="200,160">
			<animations>
				<animation name="idle" frames="20" rows="5" cols="4" width="40" height="40" background="0xE2E2E2"/>
				<animation name="blah" body="bouncy" frames="20" rows="5" cols="4" width="40" height="40" background="0xE2E2E2"/>
			</animations>
		</area>
		<area name="spinnersheet" body="spinner" topleft="0,0" bottomright="200,160"></area>
	</areas>
	<sprites>
		<sprite name="normal">
			<area name="spinnersprite"/>
			<body name="spinnerbody"/>
		</sprite>
		<sprite name="bouncy">
			<area name="spinnersprite"/>
			<body name="bouncybody"/>
		</sprite>
	</sprites>
</spritesheet>